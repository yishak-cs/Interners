{
"version":2,
    "framework": null,

    "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php",
      "config": {
        "installCommand": "apt-get update && apt-get install -y libssl1.0.0"
      }
    }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://your.url.from.vercel.app",
        "APP_KEY": "base64:gk0QFmmEcWzGXb9MjZ3VOmucKW7eISjiFdaOuPXcuFQ=",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
